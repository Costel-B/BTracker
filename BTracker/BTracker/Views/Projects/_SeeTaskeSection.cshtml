@model BTracker.Models.Taske

@if(Model.TaskeName != "EmptyTaskeNullTaske1.2.3.4.5")
{ 
    <div>
        <h4>Taske</h4>
        <hr />
        <dl class="row">
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.TaskeName)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.TaskeName)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Date)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Date)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Section)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Section.SectionName)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.TaskeState)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.TaskeState.TaskeStateName)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.TaskePriority)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.TaskePriority.TaskePriorityName)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.User)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.User.Email)
            </dd>
        </dl>
    </div>
    <div>
        @Html.ActionLink("Edit Taske", "Edit", "Taskes", new { id = Model.Section.ProjectId, sectionId = Model.SectionId, taskeId = Model.TaskeId })
    </div>
}

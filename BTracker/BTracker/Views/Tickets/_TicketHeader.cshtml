@model BTracker.Models.Ticket

<div class="dis-flex justify-content-between align-items-center">
    <ul class="navbar-nav mb-2 mb-lg-0">
        <li class="nav-link" style="padding: 0;">
            <h2 class="title">"@Html.DisplayFor(item => item.TicketName)" Details Ticket</h2>
        </li>
    </ul>
    <ul>
        <li class="nav-link dis-flex" style="padding: 0; align-items: center">
            <a id="indexProjectLink" asp-action="Edit" asp-route-id="@Model.Section.ProjectId" asp-route-sectionId="@Model.SectionId" asp-route-ticketId="@Model.TicketId" asp-route-place="@ViewData["Title"]">Edit Ticket</a>
            <a id="indexProjectLink" asp-action="Delete" asp-route-id="@Model.Section.ProjectId" asp-route-sectionId="@Model.SectionId" asp-route-ticketId="@Model.TicketId" asp-route-place="@ViewData["Title"]">Delete Ticket</a>
        </li>
    </ul>
</div>